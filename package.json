{
  "name": "react-playable",
  "version": "1.5.1",
  "description": "React wrapper for playable",
  "keywords": [
    "react",
    "adaptive",
    "dash",
    "hls",
    "html5",
    "media",
    "mpeg",
    "mse",
    "player",
    "video"
  ],
  "license": "MIT",
  "homepage": "https://github.com/wix-incubator/react-playable",
  "bugs": {
    "url": "https://github.com/wix-incubator/react-playable/issues",
    "email": "oleksiim@wix.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:wix-incubator/react-playable.git"
  },
  "main": "dist/src/index.js",
  "files": [
    "dist",
    "src",
    "testkit"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "pretest": "yoshi build && build-storybook",
    "test": "",
    "posttest": "yoshi lint",
    "postversion": "git push && git push --tags",
    "start": "yoshi start & npm run storybook",
    "build": "yoshi build",
    "storybook": "start-storybook -p 6006"
  },
  "peerDependencies": {
    "playable": ">=2.0.0",
    "react": ">=16.1.0",
    "react-dom": ">=16.1.0"
  },
  "dependencies": {
    "playable": "^2.0.0",
    "react-is": "^16.7.0"
  },
  "devDependencies": {
    "@storybook/react": "^4.0.0-alpha.0",
    "@storybook/storybook-deployer": "^2.0.0",
    "@types/chai": "^4.0.0",
    "@types/mocha": "^2.2.0",
    "@types/node": "^8.0.0",
    "@types/prop-types": "~15.5.0",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/react-is": "^16.7.0",
    "chai": "^4.1.0",
    "enzyme": "^2.9.0",
    "gh-pages": "^1.2.0",
    "husky": "~0.14.0",
    "jsdom": "~11.1.0",
    "lint-staged": "^7.2.2",
    "prop-types": "~15.6.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-test-renderer": "~16.5.1",
    "typescript": "~3.0.1",
    "yoshi": "^3.0.0",
    "yoshi-style-dependencies": "^3.0.0"
  },
  "publishConfig": {
    "logLevel": "verbose",
    "registry": "https://registry.npmjs.org/"
  },
  "lint-staged": {
    "*.tsx": [
      "prettier --write",
      "yoshi lintt --",
      "git add"
    ],
    "*.ts": [
      "prettier --write",
      "yoshi lint --",
      "git add"
    ]
  },
  "yoshi": {
    "entry": {
      "app": "index.ts"
    },
    "externals": {
      "react": "React",
      "react-dom": "ReactDOM",
      "playable": "playable"
    },
    "hmr": "auto"
  }
}
